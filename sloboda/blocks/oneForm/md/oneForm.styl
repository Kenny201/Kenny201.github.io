+media-breakpoint-up(md) {
    .oneForm{
        
        height:300px;
        &__wrap{
            background: url(form-back-catalog-new.jpg) no-repeat 100% 0%;
            background-size:100% 80%;
            height:300px;
            
           padding-top: 30px;
            p{
                color:#fff;
                text-align:left;
                font-size:20px;
               
            }
        }
        &__header {
            font-size:25px;
            color:#fff;
            line-height:35px;
            font-weight:900;
            text-align:left;
           

        }
        &__form{
               background: transparent;
               justify-content: space-between;
               box-shadow:none;
               margin-top:45px;
               display:flex;
               padding:0;
              
           input{
                outline:none;
                width:210px;
                height:50px;
                border-radius:5px;
                &:nth-child(1){
                    margin-bottom:0;
                     border:2px solid #d8d8d8;
                     &[placeholder]{
                         padding-left:20px;
                          font-size:14px;
                     }
                }
                 &:nth-child(2){
                    margin-bottom:0;
                     border:2px solid #d8d8d8;
                       &[placeholder]{
                         padding-left:20px;
                         font-size:14px;
                     }
                }
                 &:nth-child(3){
                   background: #FF7900;
                   font-size:18px;
                   text-align:center;
                   border:none;
                   margin-bottom:0;
                   box-shadow:0 15px 30px rgba(0,0,0,0.2);
                   color:#fff;
                }

            }
        }
    }

}