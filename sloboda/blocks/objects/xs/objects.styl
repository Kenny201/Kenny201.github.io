+media-breakpoint-up(xs) {
    .objects{
        margin-top:50px;
           .line-left{
                display:block;
                float:left;
                width:5%;
                
            }
            .line-right{
                display:block;
                float:right;
                width:5%;
            }
             &__header{
                 h2{
                     text-transform: uppercase;
                     color:#545352;
                     text-align: center;
                     font-size:20px;
                     font-weight:normal;
                     margin-bottom:30px;
                     span{
                         color:#C1771F;
                     }
                 }
             }
       
            &__slider{
                text-align: center;

                &_slide{
                    margin: 0 auto;
                     display: block;
                    a{
                        text-decoration: none;
                        color:#525252;
                    }
                    img{
                        margin-bottom:30px;
                        display:block;
                        width:100%;
                       
                    }

                }
                
                }
                &__button{

                    a{
                        text-align: center;
                    display: block;
                    padding:15px;
                    border:3px solid #4E4D4D;
                    border-radius:5px;
                    text-transform: uppercase;
                    text-decoration: none;
                    color:#4E4D4D;
                    }
                }
    }

}